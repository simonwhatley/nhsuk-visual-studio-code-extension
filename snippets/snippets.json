	"Address": {
		"prefix": "nhsuk-address",
		"body": [
			"{% call fieldset({",
			"\tlegend: {",
			"\t\ttext: \"${1:Address}\",",
      "\t\tisPageHeading: ${2:true},",
			"\t\tclasses: \"nhsuk-fieldset__legend--${3:l}\"",
			"\t}",
			"}) %}",
			"",
			"\t{{ input({",
			"\t\tid: \"address-line-1\",",
			"\t\tname: \"${4:address[line1]}\",",
			"\t\tlabel: {",
			"\t\t\ttext: \"Address line 1\"",
			"\t\t},",
      "\t\tvalue: ${5:data.address.line1},",
      "\t\tautocomplete: \"address-line1\"",
			"\t}) }}",
			"",
			"\t{{ input({",
			"\t\tid: \"address-line-2\",",
			"\t\tname: \"${6:address[line2]}\",",
			"\t\tlabel: {",
			"\t\t\ttext: \"Address line 2 (optional)\"",
			"\t\t},",
      "\t\tvalue: ${7:data.address.line2},",
      "\t\tautocomplete: \"address-line2\"",
			"\t}) }}",
			"",
			"\t{{ input({",
			"\t\tid: \"address-town\",",
			"\t\tname: \"${8:address[town]}\",",
			"\t\tlabel: {",
			"\t\t\ttext: \"Town or city\"",
			"\t\t},",
      "\t\tvalue: ${9:data.address.town},",
      "\t\tautocomplete: \"address-level2\",",
			"\t\tclasses: \"nhsuk-u-width-two-thirds\"",
			"\t}) }}",
			"",
			"\t{{ input({",
			"\t\tid: \"address-county\",",
			"\t\tname: \"${10:address[county]}\",",
			"\t\tlabel: {",
			"\t\t\ttext: \"County (optional)\"",
			"\t\t},",
      "\t\tvalue: ${11:data.address.county},",
			"\t\tclasses: \"nhsuk-u-width-two-thirds\"",
			"\t}) }}",
			"",
			"\t{{ input({",
			"\t\tid: \"address-postcode\",",
			"\t\tname: \"${12:address[postcode]}\",",
			"\t\tlabel: {",
			"\t\t\ttext: \"Postcode\"",
			"\t\t},",
      "\t\tvalue: ${13:data.address.postcode},",
      "\t\tautocomplete: \"postal-code\",",
			"\t\tclasses: \"nhsuk-input--width-10\"",
			"\t}) }}",
			"",
			"{% endcall %}$0"
		],
		"description": "Addresses – NHS.UK Design System"
	},
	"Bank details": {
		"prefix": "nhsuk-bank-details",
		"body": [
			"{% call fieldset({",
      "\tlegend: {",
      "\t\ttext: \"${1:Bank or building society account details}\",",
      "\t\tclasses: \"nhsuk-fieldset__legend--${2:xl}\",",
      "\t\tisPageHeading: ${3:true}",
      "\t}",
      "}) %}",
      "",
      "\t{{ input({",
      "\t\tid: \"name-on-the-account\",",
      "\t\tname: \"${4:bankDetails[nameOnAccount]}\",",
      "\t\tlabel: {",
      "\t\t\ttext: \"Name on the account\"",
      "\t\t},",
      "\t\tvalue: ${5:data.bankDetails.nameOnAccount},",
      "\t\tattributes: {",
      "\t\t\tspellcheck: \"false\"",
      "\t\t}",
      "\t}) }}",
      "",
      "\t{{ input({",
      "\t\tid: \"sort-code\",",
      "\t\tname: \"${6:bankDetails[sortCode]}\",",
      "\t\tlabel: {",
      "\t\t\ttext: \"Sort code\"",
      "\t\t},",
      "\t\thint: {",
      "\t\t\ttext: \"Must be 6 digits long\"",
      "\t\t},",
      "\t\tvalue: ${7:data.bankDetails.sortCode},",
      "\t\tclasses: \"nhsuk-input--width-5\",",
      "\t\tinputmode: \"numeric\",",
      "\t\tpattern: \"[0-9]*\",",
      "\t\tattributes: {",
      "\t\t\tspellcheck: \"false\"",
      "\t\t}",
      "\t}) }}",
      "",
      "\t{{ input({",
      "\t\tid: \"account-number\",",
      "\t\tname: \"${8:bankDetails[accountNumber]}\",",
      "\t\tlabel: {",
      "\t\t\ttext: \"Account number\"",
      "\t\t},",
      "\t\thint: {",
      "\t\t\ttext: \"Must be between 6 and 8 digits long\"",
      "\t\t},",
      "\t\tvalue: ${9:data.bankDetails.accountNumber},",
      "\t\tclasses: \"nhsuk-input--width-10\",",
      "\t\tinputmode: \"numeric\",",
      "\t\tpattern: \"[0-9]*\",",
      "\t\tattributes: {",
      "\t\t\tspellcheck: \"false\"",
      "\t\t}",
      "\t}) }}",
      "",
      "\t{{ input({",
      "\t\tid: \"roll-number\",",
      "\t\tname: \"${10:bankDetails[rollNumber]}\",",
      "\t\tlabel: {",
      "\t\t\ttext: \"Building society roll number (if you have one)\"",
      "\t\t},",
      "\t\thint: {",
      "\t\t\ttext: \"You can find it on your card, statement or passbook\"",
      "\t\t},",
      "\t\tvalue: ${11:data.bankDetails.rollNumber},",
      "\t\tclasses: \"nhsuk-input--width-10\",",
      "\t\tattributes: {",
      "\t\t\tspellcheck: \"false\"",
      "\t\t}",
      "\t) }}",
      "",
      "{% endcall %}$0"
		],
		"description": "Bank details – NHS.UK Design System"
	},
	"Date input": {
		"prefix": "nhsuk-date",
		"body": [
			"{{ dateInput({",
			"\tid: \"${1:date}\",",
			"\tfieldset: {",
			"\t\tlegend: {",
			"\t\t\ttext: \"${2:Legend text}\",",
			"\t\t\tisPageHeading: ${3:true},",
			"\t\t\tclasses: \"nhsuk-fieldset__legend--${4:xl}\"",
			"\t\t}",
			"\t},",
			"\thint: {",
			"\t\ttext: \"${5:For example, 31 3 1980}\"",
			"\t},",
			"\titems: [",
			"\t\t{",
			"\t\t\tname: \"${1:date}$6\",",
      "\t\t\tlabel: \"Day\",",
      "\t\t\tvalue: data.${7:day},",
      "\t\t\tclasses: \"nhsuk-input--width-2\"",
			"\t\t},",
			"\t\t{",
			"\t\t\tname: \"${1:date}$8\",",
      "\t\t\tlabel: \"Month\",",
      "\t\t\tvalue: data.${9:month},",
      "\t\t\tclasses: \"nhsuk-input--width-2\"",
			"\t\t},",
			"\t\t{",
			"\t\t\tname: \"${1:date}$10\",",
      "\t\t\tlabel: \"Year\",",
      "\t\t\tvalue: data.${11:year},",
      "\t\t\tclasses: \"nhsuk-input--width-4\"",
			"\t\t}",
			"\t]",
			"}) }}$0"
		],
		"description": "Date input – NHS.UK Design System"
	},
