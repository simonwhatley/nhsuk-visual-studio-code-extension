	},
	"Button": {
		"prefix": "nhsuk-button",
		"body": [
			"{{ button({",
			"\ttext: \"${1:Button text}\"",
			"}) }}$0"
		],
		"description": "Button – NHS.UK Design System"
	  },
	"Checkboxes": {
		"prefix": "nhsuk-checkboxes",
		"body": [
			"{{ checkboxes({",
			"\tname: \"$1\",",
			"\tfieldset: {",
			"\t\tlegend: {",
			"\t\t\ttext: \"${2:Legend text}\",",
			"\t\t\tisPageHeading: ${3:true},",
			"\t\t\tclasses: \"nhsuk-fieldset__legend--${4:l}\"",
			"\t\t}",
			"\t},",
			"\thint: {",
			"\t\ttext: \"${5:Select all that apply}\"",
			"\t},",
      "\tvalues: data.$6,",
			"\titems: [",
      "\t\t{",
      "\t\t\tvalue: \"${7:One}\",",
      "\t\t\ttext: \"${7:One}\"",
      "\t\t},",
      "\t\t{",
      "\t\t\tvalue: \"${8:Two}\",",
      "\t\t\ttext: \"${8:Two}\"",
      "\t\t},",
      "\t\t{",
      "\t\t\tvalue: \"${9:Three}\",",
      "\t\t\ttext: \"${9:Three}\"",
      "\t\t}$10",
			"\t]",
			"}) }}$0"
		],
		"description": "Checkboxes – NHS.UK Design System"
	},
	"Radios": {
		"prefix": "nhsuk-radios",
		"body": [
			"{{ radios({",
			"\tname: \"$1\",",
			"\tfieldset: {",
			"\t\tlegend: {",
			"\t\t\ttext: \"${2:Legend text}\",",
			"\t\t\tisPageHeading: ${3:true},",
			"\t\t\tclasses: \"nhsuk-fieldset__legend--${4:l}\"",
			"\t\t}",
			"\t},",
      "\tvalue: data.$5,",
			"\titems: [",
      "\t\t{",
      "\t\t\tvalue: \"${6:Yes}\",",
      "\t\t\ttext: \"${6:Yes}\"",
			"\t\t},",
      "\t\t{",
      "\t\t\tvalue: \"${7:No}\",",
      "\t\t\ttext: \"${7:No}\"",
			"\t\t}$8",
			"\t]",
			"}) }}$0"
		],
		"description": "Radios – NHS.UK Design System"
	},
	"Select": {
		"prefix": "nhsuk-select",
		"body": [
