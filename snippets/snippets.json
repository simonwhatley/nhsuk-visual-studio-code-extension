		"prefix": "nhsuk-list",
		"body": [
			"<ul class=\"nhsuk-list\">",
			"\t<li>",
			"\t\t${1:List item}",
			"\t</li>",
			"\t<li>",
			"\t\t${2:List item}",
			"\t</li>",
			"\t<li>",
			"\t\t${3:List item}",
			"\t</li>",
			"</ul>$0"
		],
		"description": "Lists – NHS.UK Design System"
	},
	"Bulleted lists": {
		"prefix": "nhsuk-list-bullet",
		"body": [
			"<ul class=\"nhsuk-list nhsuk-list--bullet\">",
			"\t<li>",
			"\t\t${1:List item}",
			"\t</li>",
			"\t<li>",
