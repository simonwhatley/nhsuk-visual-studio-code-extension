		"body": [
			"{{ fileUpload({",
			"\tid: \"${1:file-upload-1}\",",
			"\tname: \"${1:file-upload-1}\",",
			"\tlabel: {",
			"\t\ttext: \"${2:Upload a file}\",",
      "\t\tisPageHeading: ${3:true},",
      "\t\tclasses: \"nhsuk-label--${4:xl}\"",
			"\t},",
			"\thint: {",
			"\t\ttext: \"${5:Hint text}\"",
			"\t}",
			"}) }}$0"
		],
		"description": "File upload – NHS.UK Design System"
	},
	"National Insurance number (NINO)": {
		"prefix": "nhsuk-nino",
		"body": [
